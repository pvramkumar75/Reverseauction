{
    "version": 2,
    "name": "bidflow-frontend",
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "frontend/$1"
        }
    ]
}